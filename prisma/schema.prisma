// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model root_of_equation {
  id Int @id @default(autoincrement())
  time DateTime @default(now())
  equation String
  answer Float?
  graph Json
  isOpenMethod Boolean @default(false)
  x_start Json
  x_end Float? @default(0)
  result Json @default("{}")
  tolerance Float
  type String
}

model linear_algebra {
  id Int @id @default(autoincrement())
  time DateTime @default(now())
  dimension Int
  matrixA Json
  matrixB Json
  matrixX Json
  error_criteria Float?
  result Json @default("{}")
  type String
}

model interpolation {
  id Int @id @default(autoincrement())
  time DateTime @default(now())
  numberOfPoint Int
  x Json
  y Json
  xToFind Float
  answer Float
  graph Json
  type String
}

model regression {
  id Int @id @default(autoincrement())
  time DateTime @default(now())
  numberOfPoint Int
  x Json
  y Json
  xToFind Json
  answer Float
  graph Json?
  morder Int @default(1)
  type String
}